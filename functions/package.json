{
  "name": "dc-metro-google-assistant",
  "description": "DC Metro Google Assistant action that will show you the latest train and bus arrival information for your commute.",
  "version": "0.0.1",
  "author": "James Ives",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesIves/dc-metro-google-assistant-action.git"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json src/**/*.ts -e tests/**/*.ts",
    "build": "tsc",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "tape -r ts-node/register src/tests/*.spec.ts",
    "format": "prettier --write './src/**/*.ts'"
  },
  "dependencies": {
    "actions-on-google": "^2.5.0",
    "firebase-admin": "^6.3.0",
    "firebase-functions": "^2.1.0",
    "node-fetch": "^2.3.0",
    "prettier": "*",
    "tape": "^4.6.2"
  },
  "devDependencies": {
    "@types/tape": "^4.2.32",
    "ts-node": "7.0.1",
    "tslint": "~5.8.0",
    "typescript": "~2.8.3"
  },
  "private": true,
  "keywords": [
    "washington-dc",
    "metro",
    "civic",
    "google-assistant"
  ],
  "bugs": {
    "url": "https://github.com/JamesIves/dc-metro-google-assistant-action/issues"
  },
  "homepage": "https://jamesiv.es"
}
